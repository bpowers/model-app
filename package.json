{
  "name": "model-app",
  "description": "The frontend and service for systemdynamics.net",
  "version": "0.1.0",
  "homepage": "https://systemdynamics.net",
  "main": "src",
  "private": true,
  "license": "AGPL-3.0-only",
  "keywords": [],
  "author": {
    "name": "Bobby Powers",
    "email": "bobbypowers@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "needs-format": "prettier -l \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format": "prettier --write \"**/*.ts\" \"src/**/*.tsx\"",
    "precommit": "npm-run-all -p needs-format lint",
    "install-git-hooks": "cd .git/hooks && rm -f pre-commit && ln -s ../../support/pre-commit.hook ./pre-commit",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "deploy": "export NODE_ENV=production && yarn build && cp -r build/* public && cp -r build-component/static/js/sd-component* ./public/static/js/ && gcloud app deploy ./.app.prod.yaml && git checkout HEAD -- public/index.html && rm -r public/asset-manifest.json public/static build build-component lib",
    "build": "npm-run-all -p build:backend build:frontend build:webcomponent",
    "build:backend": "tsc",
    "build:frontend": "node scripts/build.js",
    "build:webcomponent": "node scripts/build-component.js",
    "build:pre": "node support/mkdirp.js build-rt lib build",
    "build:runtime0": "tsc -p .tsconfig.rt.json",
    "build:runtime1": "node support/build-runtime.js src/engine/runtime.ts",
    "build:runtime": "npm-run-all build:pre build:runtime0 build:runtime1",
    "build:gen-protobufs": "protoc --plugin='protoc-gen-ts=node_modules/.bin/protoc-gen-ts' --js_out='import_style=commonjs,binary:.' --ts_out=. $(find src -name '*.proto')",
    "test": "tsc -p .tsconfig.test.json && mocha",
    "rtest": "./test/test-models/regression-test.py -x '(subscript|macro|xscale|initial_function|stocks_with_expressions|active_initial|smooth_and_stock|euler_step_vs_saveper|special_characters)' ./bin/mdl.js test/test-models",
    "mocha": "mocha test/ --recursive --exit",
    "start": "node lib",
    "start:firestore": "gcloud beta emulators firestore start --host-port=127.0.0.1:8092",
    "start:testing": "npm-run-all -p start:backend start:frontend",
    "start:backend": "export FIRESTORE_EMULATOR_HOST=127.0.0.1:8092; exec node_modules/.bin/ts-node-dev --no-notify --respawn src/index.ts",
    "start:frontend": "node scripts/start.js",
    "test:backend": "yarn run build && yarn run mocha",
    "test:frontend": "node scripts/test.js"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.1.1",
    "@google-cloud/trace-agent": "^5.0.0",
    "@iarna/toml": "^2.2.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "canvas": "^2.6.0",
    "canvg": "^3.0.0-beta.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "google-protobuf": "^3.10.0",
    "helmet": "^3.21.1",
    "immutable": "^4.0.0-rc.12",
    "jsdom": "^16.1.0",
    "mongodb": "^3.3.4",
    "mustache": "^4.0.0",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "recharts": "2.0.0-beta.3",
    "saslprep": "^1.0.3",
    "serve-favicon": "^2.5.0",
    "seshcookie": "^1.0.1",
    "slate": "^0.58.1",
    "slate-history": "^0.58.1",
    "slate-react": "^0.58.1",
    "uuid": "^8.0.0",
    "winston": "^3.2.1",
    "xmldom": "^0.3.0"
  },
  "resolutions": {
    "@types/slate*/**/immutable": "4.0.0-rc.12"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.6.0",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.7.0",
    "@types/chai": "^4.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/google-protobuf": "^3.7.2",
    "@types/helmet": "^0.0.47",
    "@types/mocha": "^8.0.0",
    "@types/mongodb": "^3.3.10",
    "@types/mustache": "^4.0.0",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.3",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/recharts": "^1.8.3",
    "@types/serve-favicon": "^2.2.31",
    "@types/uuid": "^8.0.0",
    "@types/xmldom": "^0.1.29",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "bfj": "^7.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.1.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "identity-obj-proxy": "^3.0.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^26.0.1",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "pnp-webpack-plugin": "^1.5.0",
    "prettier": "^2.0.1",
    "react-dev-utils": "10.1.0",
    "react-svg-loader": "^3.0.3",
    "resolve": "^1.12.0",
    "terser-webpack-plugin": "^3.0.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.49",
    "ts-protoc-gen": "^0.12.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.41.1",
    "webpack-dev-server": "^3.8.2",
    "webpack-manifest-plugin": "^2.2.0",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari versions",
    "last 1 ios_saf versions"
  ],
  "proxy": "http://localhost:3030",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/typescript",
      {
        "exclude": [
          "transform-runtime",
          "transform-regenerator"
        ]
      }
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-optional-chaining",
        {}
      ]
    ]
  }
}
