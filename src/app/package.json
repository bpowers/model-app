{
  "name": "@system-dynamics/app",
  "version": "1.0.0",
  "description": "Simlin web frontend",
  "repository": "http://github.com/bpowers/simlin",
  "author": "Bobby Powers <bobbypowers@gmail.com>",
  "license": "Apache-2.0",
  "browser": "lib.browser",
  "dependencies": {
    "@system-dynamics/core": "^1.0.0",
    "@system-dynamics/diagram": "^1.0.0",
    "@system-dynamics/importer": "^1.0.0",
    "@system-dynamics/xmutil": "^1.0.0",
    "google-protobuf": "^3.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/eslint-parser": "^7.13.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "csp-html-webpack-plugin": "^5.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.0.1",
    "react-dev-utils": "^11.0.1",
    "react-refresh": "^0.9.0",
    "resolve": "^1.19.0",
    "resolve-url-loader": "^3.1.2",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "~4.44.2",
    "webpack-dev-server": "^3.11.1",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "resolutions": {
    "immer": "8.0.1"
  },
  "scripts": {
    "needs-format": "prettier -l '**/*.ts' '**/*.tsx'",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "lint": "eslint '**/*.ts' '**/*.tsx'",
    "prepublishOnly": "yarn build",
    "deploy": "cp -r build/* public && cp -r build-component/static/js/* ./public/static/js/ && cp -r build-component/static/wasm/* ./public/static/wasm/",
    "deploy-clean": "git checkout HEAD -- ./public/index.html && rm -r public/asset-manifest.json public/static/js public/static/wasm build build-component",
    "clean": "rm -rf ./lib.browser ./build ./build-component",
    "build": "yarn build:frontend && yarn build:webcomponent",
    "build:tsc": "tsc -p tsconfig.browser.json",
    "build:webcomponent": "node scripts/build-component.js",
    "build:frontend": "node scripts/build.js",
    "start:frontend": "node scripts/start.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari versions",
    "last 1 ios_saf versions"
  ],
  "proxy": "http://localhost:3030"
}
