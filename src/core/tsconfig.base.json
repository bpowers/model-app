{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
}
